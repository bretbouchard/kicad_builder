(module 74AHCT125_SOT23-5 (layer F.Cu) (tedit 64B2A203)
  (descr "SOT-23-5 footprint â€” updated to common KiCad-style dimensions; verify against SN74AHCT125 datasheet before production")
  (tags "74AHCT125 SOT-23-5")
  (fp_text reference U (at 0 0) (layer F.SilkS))
  (fp_text value 74AHCT125 (at 0 -2.0) (layer F.Fab))

  ;; Pad geometry updated to common SOT-23-5 landpattern (units mm):
  ;; pin pitch (between pins on same side) ~0.95 mm, pad width ~0.6 mm, pad length ~1.5 mm for top pins,
  ;; and larger bottom pads length ~1.9 mm to aid solderability.
  (pad 1 smd rect (at -1.9 0.9) (size 0.6 1.5) (layers F.Cu F.Paste F.Mask))
  (pad 2 smd rect (at 0.0 0.9) (size 0.6 1.5) (layers F.Cu F.Paste F.Mask))
  (pad 3 smd rect (at 1.9 0.9) (size 0.6 1.5) (layers F.Cu F.Paste F.Mask))

  (pad 4 smd rect (at -0.95 -1.35) (size 0.6 1.9) (layers F.Cu F.Paste F.Mask))
  (pad 5 smd rect (at 0.95 -1.35) (size 0.6 1.9) (layers F.Cu F.Paste F.Mask))

  ;; Silkscreen outline
  (fp_line (start -3.0 2.2) (end 3.0 2.2) (layer F.SilkS) (width 0.12))
  (fp_line (start 3.0 2.2) (end 3.0 -2.2) (layer F.SilkS) (width 0.12))
  (fp_line (start 3.0 -2.2) (end -3.0 -2.2) (layer F.SilkS) (width 0.12))
  (fp_line (start -3.0 -2.2) (end -3.0 2.2) (layer F.SilkS) (width 0.12))
)
